{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/",
      "destination": "/public/openapi.json"
    }
  ],
  "functions": {
    "api/*.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "api/util/*.*"
    }
  },
  "public": true
}
